{
    "name": "weweb_landing_hero",
    "type": "section",
    "wewebPublic": "true",
    "repository": {
        "type": "git",
        "url": "https://github.com/WeWeb-Public/weweb-landing-hero"
    },
    "version": "0.0.4",
    "private": true,
    "scripts": {
        "build": "weweb-build",
        "serve": "weweb-serve",
        "create-config": "weweb-config",
        "lint:fix": "eslint src/*.{js,vue} --fix",
        "lint": "eslint src/*.{js,vue}",
        "test": "lint-staged",
        "prettify": "prettier src/*.{js,vue} --write"
    },
    "dependencies": {
        "vue": "2.6.11"
    },
    "devDependencies": {
        "@vue/eslint-config-prettier": "^6.0.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^7.2.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-import": "^2.21.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.1",
        "eslint-plugin-vue": "^6.2.2",
        "husky": "^4.2.5",
        "lint-staged": "^10.2.10",
        "prettier": "^2.0.5",
        "stylelint": "^13.6.0",
        "stylelint-config-standard": "^20.0.0",
        "weweb-client": "latest"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.(js|vue|html|css)": [
            "prettier --write",
            "git add"
        ],
        "*.(js|vue)": [
            "npm run lint:fix",
            "npm run lint"
        ]
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
